[{
  "include": [ "~/.config/waybar/modules" ],

  "name": "notebook-main",
  "layer": "top",
  "position": "top",
  "ipc": true,
  "output": "eDP-1",

  "modules-left": [
      "sway/workspaces",
      "sway/window"
  ],
  "modules-center": [
      "sway/mode"
  ],
  "modules-right": [
    "mpris",
    "backlight",
    "battery",
    "pulseaudio",
    "clock",
    "tray"
  ]
}, {
  "include": [ "~/.config/waybar/modules" ],

  "name": "notebook-secondary",
  "layer": "top",
  "position": "top",
  "ipc": true,
  "output": "DP-4",

  "modules-left": [
    "mpris"
  ],
  "modules-center": [
  ],
  "modules-right": [
    "clock"
  ]
}, {
  "include": [ "~/.config/waybar/modules" ],

  "name": "desktop-main",
  "layer": "top",
  "position": "top",
  "ipc": true,
  "output": "HDMI-A-1",

  "modules-left": [
      "sway/workspaces",
      "sway/window"
  ],
  "modules-center": [
      "sway/mode"
  ],
  "modules-right": [
    "mpris",
    "backlight",
    "battery",
    "pulseaudio",
    "clock",
    "tray"
  ]

}, {
  "include": [ "~/.config/waybar/modules" ],

  "name": "desktop-secondary",
