# vim: filetype=swayconfig

set $mod Mod4

bindsym {
    $mod+p output eDP-1 toggle
}

output eDP-1 {
  mode 2256x1504
  position 0 0
  scale 1
}

workspace {
  number 1
  number 2
  number 3
  number 4
  number 5
  number 6
  number 8
}

workspace {
  1 output eDP-1
  2 output eDP-1
  3 output eDP-1
  4 output eDP-1
  5 output eDP-1
  6 output eDP-1
  8 output eDP-1
}
